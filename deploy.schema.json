{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://cdn.jsdelivr.net/gh/moj-analytical-services/deploy.json/deploy.schema.json",
  "type": "object",
  "properties": {
    "mojanalytics-deploy": {
      "type": "string"
    },
    "disable_authentication": {
      "type": "boolean"
    },
    "allowed_ip_ranges": {
      "type": "array",
      "items": {
        "type": "string",
        "pattern": ""
      },
      "minItems": 1
    },
    "type": {
      "type": "string",
      "default": "webapp",
      "enum": [
        "airflow_dag",
        "webapp",
        "docker_build",
        "lookup"
      ]
    },
    "role_name": {
      "type": "string"
    },
    "port": {
      "type": "string"
    }
  },
  "required": [
    "mojanalytics-deploy"
  ]
}
